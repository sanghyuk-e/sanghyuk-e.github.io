<span class="ni">&amp;lt;</span>rss version=<span class="ni">&amp;quot;</span>2.0<span class="ni">&amp;quot;</span> xmlns:atom=<span class="ni">&amp;quot;</span>http://www.w3.org/2005/Atom<span class="ni">&amp;quot;&amp;gt;</span>
  <span class="ni">&amp;lt;</span>channel<span class="ni">&amp;gt;</span>
    <span class="ni">&amp;lt;</span>title<span class="ni">&amp;gt;</span>{{ site.title | xml_escape }}<span class="ni">&amp;lt;</span>/title<span class="ni">&amp;gt;</span>
    <span class="ni">&amp;lt;</span>description<span class="ni">&amp;gt;</span>{{ site.description | xml_escape }}<span class="ni">&amp;lt;</span>/description<span class="ni">&amp;gt;</span>
